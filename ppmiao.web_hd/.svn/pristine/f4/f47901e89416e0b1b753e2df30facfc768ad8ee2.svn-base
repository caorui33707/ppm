<script type="text/javascript">
    var _chart = null;
    var _t = '{$request_verify.time}';
    var _v = '{$request_verify.verify}';

    Highcharts.setOptions({
        lang: {
            loading: '正在加载数据...'
        },
        colors: ['#7cb5ec', '#FF0000', '#B34EE9']
    });
    $('#container').highcharts({
        chart: {
            events: {
                load: function(e) {
                    _chart = this;
                    getChart();
                }
            }
        },
        title: {
            text: null
        },
        subtitle: {
            text: null
        },
        credits: {
            enabled: false
        },
        tooltip: {
            shared: true,
            formatter: function() {
                var _dateArr = new Date(this.x).toLocaleString().split(' ');
                return _dateArr[0].replace('年',"-").replace('月',"-").replace('日',"")+'<br><span style="color:#7cb5ec;">公募基金累计净值:'+this.points[0].y+'</span><br><span style="color:#FF0000;">A类净值:'+this.points[1].y+'</span><br><span style="color:B34EE9;">B类净值:'+this.points[2].y+'</span>';
            }
        },
        legend: {
            enabled: true,
            align: 'left',
            verticalAlign: 'top'
        },
        plotOptions: {
            area: {
                fillColor: {
                    linearGradient: { x1: 0, y1: 0, x2: 0, y2: 1},
                    stops: [
                        [0, Highcharts.getOptions().colors[0]],
                        [1, Highcharts.Color(Highcharts.getOptions().colors[0]).setOpacity(0).get('rgba')]
                    ]
                },
                lineWidth: 1,
                marker: {
                    enabled: false
                },
                shadow: false,
                states: {
                    hover: {
                        lineWidth: 1
                    }
                },
                threshold: null
            },
            series: {
                marker: {
                    enabled: false
                },
                events: {
                    legendItemClick: function(){
                        return false;
                    }
                }
            }
        },
        xAxis: {
            type: 'datetime',
            labels: {
                format: '{value:%m-%d}',
                align: 'center'
            }
        },
        yAxis: [{
            lineWidth: 0,
            title: {
                text: null
            }
        }],
        series: [{
            type: 'line',
            name: '公募基金累计净值',
            data: []
        },{
            name: 'A类净值',
            type: 'line',
            data: []
        },{
            name: 'B类净值',
            type: 'line',
            data: []
        }]
    });

    function getChart() {
        _chart.showLoading();
        $.post("{:C('WEB_ROOT')}/api/getFundData", {id: 2, t: _t, v: _v}, function (msg) {
            if (msg.status) {
                var _basedata = new Array();
                var _adata = new Array();
                var _bdata = new Array();
                $.each(msg.base_arr, function(i, n){
                    _basedata.push([n.datetime, parseFloat(n.val)]);
                });
                $.each(msg.a_arr, function(i, n){
                    _adata.push([n.datetime, parseFloat(n.val)]);
                });
                $.each(msg.b_arr, function(i, n){
                    _bdata.push([n.datetime, parseFloat(n.val)]);
                });
                _chart.series[0].setData(_basedata);
                _chart.series[1].setData(_adata);
                _chart.series[2].setData(_bdata);
                _chart.hideLoading();
            }
        });
    }
</script>
