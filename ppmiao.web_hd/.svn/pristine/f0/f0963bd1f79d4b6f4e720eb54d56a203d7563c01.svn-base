.main {
    background-color: #F5F5F5;
    overflow: hidden;
    margin-top: 96px;
}

.data-container {
    width: 1200px;
    height: auto;
    margin: 40px auto;
    padding: 10px 0;
    background-color: #fff;
    border: 1px solid #d9d9d9;
}

.details-container {
    width: 1200px;
    height: auto;
    margin: 40px auto;
    padding: 10px 0;
    /* background-color: #fff; */
    /* border: 1px solid #d9d9d9; */
}

.details {
    padding: 0 40px;
    position: relative;
}

.title {
    height: 28px;
    line-height: 28px;
}

.title-name {
    font-size: 16px;
    color: #333;
    font-weight: bold;
    margin-right: 30px;
}

.title-bankIcon {
    height: 17px;
    position: relative;
    top: 3px;
}

.dataDetails {
    display: flex;
    flex-wrap: wrap;
}

.data-detail__num {
    width: 55%;
    display: inherit;
    border-bottom: 1px dashed #d9d9d9;
    padding: 20px 0;
}

.detail-earnings__num {
    font-size: 36px;
    color: #ff5813;
}

.detail-earnings__text {
    font-size: 14px;
    color: #999;
}

.detail-lineheight {
    line-height: 35px;
    width: 200px;
}

.detail-duration__num {
    font-size: 36px;
    color: #000;
}

.detail-duration__text {
    font-size: 14px;
    color: #999;
    margin-top: 8px;
}

.detail-content {
    display: flex;
    font-size: 14px;
    color: #999;
    padding: 10px 0;
}

.detail-content div {
    margin-right: 160px;
    width: 170px;
}

.detail-content__progress {
    width: 72px !important;
    margin-right: 9px !important;
}

.buy {
    position: absolute;
    background-color: #fff5f1;
    top: 63px;
    bottom: 30px;
    left: 700px;
    right: 40px;
    font-size: 14px;
    color: #999;
}

.buy__restMoney {
    margin-bottom: 10px;
}

.buy__restMoney span {
    color: #ff580f;
}

.buy__accountMoney {
    margin: 20px 0;
}

.buy_recharge {
    color: #ff580f;
    margin-left: 20px;
}

.buy__shopping {
    width: 350px;
    background-color: #ff580f;
    color: #fff;
    margin: 30px auto 20px auto;
    border-radius: 5px;
    height: 50px;
    line-height: 50px;
    text-align: center;
}

.buy__up {
    position: relative;
    left: 0px;
    font-size: 14px;
    color: #999;
}

.vessel {
    border-left: 1px solid #d9d9d9;
    border-right: 1px solid #d9d9d9;
    border-bottom: 1px solid #d9d9d9;
    padding: 0 40px;
    background-color: #fff;
    font-size: 14px;
}

.vessel-part {
    border-bottom: 1px solid #d9d9d9;
}

.details-title {
    padding: 30px 0;
    color: #ff580f;
}

.details-text {
    padding: 0 0 30px 0;
    color: #999;
}

.ui.tabular.menu {
    margin-bottom: 0 !important;
}

.details-img img {
    width: 100px;
    height: 100px;
    margin: 0 5px 20px 5px;
}

.ui.tabular.menu .item {
    background-color: #fff;
    border-top: 1px solid #d4d4d5;
    border-right: 1px solid #d4d4d5;
    border-left: 1px solid #d4d4d5;
    border-radius: .28571429rem .28571429rem 0 0 !important;
}

.risk {
    position: absolute;
    right: 40px;
    bottom: 1px;
    font-size: 14px;
    color: #999;
}

.exchangeFirst {
    text-decoration: underline;
    color: #ff580f;
}

.bigPic {
    position: fixed;
    top: 0px;
    bottom: 0;
    left: 0;
    right: 0;
}

.bigPic-shadow {
    background: #000;
    opacity: .3;
    width: 100%;
    height: 100%;
}

.bigPic-Pic {
    width: 600px;
    /* height: 600px; */
    top: 20%;
    left: 27%;
    position: absolute;
}
.dialog {
    position: fixed;
    top: 0px;
    bottom: 0px;
    left: 0px;
    right: 0px;
}

.dialog-shadow {
    background-color: #000;
    opacity: .3;
    width: 100%;
    height: 100%;
}

.dialog-container {
    width: 400px;
    background: #fff;
    height: 340px;
    margin: -200px auto 0 auto;
    position: relative;
    top: -50%;
    border-radius: 5px;
}

.dialog-container__text {
    font-size: 22px;
    text-align: center;
    position: relative;
    top: 106px;
}
.dialog-container__button {
    position: relative;
    width: 300px;
    background: #ff580f;
    margin: 0 auto;
    color: #fff;
    height: 50px;
    line-height: 50px;
    border-radius: 5px;
    text-align: center;
    font-size: 20px;
    top: 180px;
}
.img-overflow {
    overflow: auto;
    height: 600px;
    width: 900px;
    position: absolute;
    top: 20%;
    left: 50%;
    margin-left: -514px;
}

.allin {
    position: absolute;
    left: 211px;
    top: 10px;
    color: #ff580f;
}
.noRecord {
    background-image: url(../images/noRecord.png);
    background-repeat: no-repeat;
    background-size: 100%;
    width: 100px;
    height: 173px;
    position: relative;
    left: 50%;
    margin-left: -70px;
    top: 59px;
}
