<!DOCTYPE HTML PUBLIC "-//WAPFORUM//DTD XHTML Mobile 1.0//EN" >
<html>
<head>
    <meta charset="UTF-8" name="viewport" content="width=device-width,initial-scale=1.0,maximum-scale=1.0,user-scalable=0" />
    <link href="https://image.ppmiao.com/Public/activity/_918zs/css/style.css" rel="stylesheet" type="text/css" />
    <script src="https://image.ppmiao.com/Public/activity/_918zs/js/jquery.min.js"></script>
    <script src="https://image.ppmiao.com/Public/activity/_918zs/js/flexible.js"></script>
    <script src="https://image.ppmiao.com/Public/activity/_918zs/js/flexible_css.js"></script>
    <title>票票喵</title>
    <style>
    body {
        width: 100%;
        background-color:#f4f4f4;
    }
    .register_pic {
      background-image: url("https://image.ppmiao.com/Public/activity/_918zs/images/card1.jpg");
      background-repeat: no-repeat;
      background-size: 100%;
      width: 10rem;
      height: 6rem;
      text-align: center;
      margin: auto;
    }
    .register_ppm{
      background-image: url("https://image.ppmiao.com/Public/activity/_918zs/images/card2.jpg");
      background-repeat: no-repeat;
      background-size: 100%;
      width: 10rem;
      height: 5.7rem;
      text-align: center;
      margin: auto;
      position: relative;
    }
    </style>

</head>
<body>
    <section>
        <!--弹出层-->
        <div class="backgr" id="integral_bj" style="display: none"></div>
        <div class="tankuang1 rel" id="integral_window" style="z-index:12;display: none;top:3%;left:10%;">
            <div class="integral_content1 abs" style="top:3%;left:10%;">
            	<if condition="($device_type eq 'ios')" > 
                <img src="https://image.ppmiao.com/Public/activity/_918zs/images/ios_03.png" />
				<else />
                <img src="https://image.ppmiao.com/Public/activity/_918zs/images/andrio.png" />
                </if>
            </div>
        </div>
        <!--正文-->
        <div class="register_pic"></div>
        <div class="register_ppm">
          <div class="register_small">
              <img src="https://image.ppmiao.com/Public/activity/_918zs/images/small_09.png"  style="width:0.6rem;"/>
          </div>
          <if condition="($type eq 1)"> 
          <div>
            <div class="register_user">
                <p>您已是票票喵理财用户</p>
            </div>
            <div class="register_you">
              <p>超高年化收益在等您！</p>
            </div>
          </div>
          <else/>
          <div>
            <div class="register_user">
                <p>恭喜您注册成功</p>
            </div>
            <div class="register_you1">
              <p><span style="font-size:0.6rem;font-weight:blod;color:#ff6905;">888</span><a style="color:#ff6905;">元</a>红包等您领！</p>
            </div>
          </div>
          </if>
        </div>
        <div class="register_download">
            <div class="login_app">
            	<if condition="($is_weixin eq 1)"> 
                	<img id="img" src="https://image.ppmiao.com/Public/activity/_918zs/images/xia_06.png"/>
                <else/>
                	<a href="{$url}"><img src="https://image.ppmiao.com/Public/activity/_918zs/images/xia_06.png"/></a>
                </if>
            </div>
        </div>
    </section>
</body>
</html>
<script>
    $(function () {
        $("#img").click(function () {
            $("#integral_bj").show();
            $("#integral_window").show();
        });
        $(".backgr").click(function () {
            $("#integral_bj").hide();
            $("#integral_window").hide();
        });
    });
</script>
