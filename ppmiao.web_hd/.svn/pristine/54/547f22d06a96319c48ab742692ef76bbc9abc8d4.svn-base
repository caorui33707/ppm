    font-size:36px;
    margin:0 3px;}
.shareTxt h5{
    font:20px/30px "微软雅黑";
    color:#ffffff;
    text-align:center;
    margin:0px 8px 0 8px;}
.shareImg01Box{
    width:100%;
    height:220px;
    position:relative;
    background:#f64349;}
.shareImg01{
    width:228px;
    height:203px;
    background:url(../images/shareImg01.png) no-repeat;
    background-size:228px 203px;
    position:absolute;
    left:50%;
    margin-left:-104px;
    top:20px;
    z-index:888;
}
.starBox{
    width:100%;
    position:absolute;
    z-index:777;}
.star{
    width:24px;
    height:28px;
    background:url(../images/star.png) no-repeat;
    background-size:24px 28px;
    position:absolute;
}
.anima{
    -webkit-animation-name: 'opt'; /*动画名称*/
    -webkit-animation-duration: 2s;/*播放一次所持续时间*/
    -webkit-animation-timing-function: ease;/*动画播放频率*/
    -webkit-animation-iteration-count: infinite;/*动画播放次涒为无限次*/}
@-webkit-keyframes 'opt' {
    from {
        opacity: .1;
    }
    20% {
        opacity: .2;
    }
    40% {
        opacity: .4;
    }
    80% {
        opacity: .8;
    }
    100% {
        opacity: 1;
    }
    to {
        opacity: .1
    }
}

.star01{
    left:50%;
    margin-left:80px;
    top:50px;
    opacity: 0;}
.star02{
    left:50%;
    margin-left:-120px;
    top:30px;
    opacity: 0;}
.star03{
    left:50%;
    margin-left:-140px;
    top:100px;
    opacity: 0;}
.shareImg02Box {
    width:100%;
