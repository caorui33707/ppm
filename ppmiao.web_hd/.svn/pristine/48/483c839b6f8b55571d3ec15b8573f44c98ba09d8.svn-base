.ui-dialog {
    background-color: transparent;
    border: 0;
}

.ui-popup-modal .ui-dialog {
    box-shadow: none;
}



.content {
	display: block;
	width: 100%;
	overflow: hidden;
	margin: 0 auto;

}

.container {
	width: 100%;
	background: #461704;
	float: left;
	display: inline-block;
	overflow: hidden;
	box-sizing: border-box;
	height: 2rem;
	position: relative;
	cursor: pointer;
}

.marquee-sibling {
	padding: 0;
	background: #461704;
	width: 10%;
	height: 2rem;
	line-height: 2rem;
	font-size: 12px;
	font-weight: normal;
	color: #ffffff;
	text-align: center;
	float: left;
	left: 0;
	z-index: 5;
}
    .marquee-sibling img {
    margin-top:0.5rem;
    width:10px;
    }

.marquee, *[class^="marquee"] {
	display: inline-block;
	white-space: nowrap;
	position:absolute;
}

.marquee{
  margin-left: 25%;
}

.marquee-content-items{
  display: inline-block;
  margin: 0;
  height: 2rem;
  position: relative;
}

.marquee-content-items li{
  display: inline-block;
  line-height: 2rem;
  color:#fff;
  margin-left:15px;
  font-size: 14px;
}

.marquee-content-items li:after{
	margin: 0 1em;
}
.ui-popup-modal .ui-dialog {
    background-color: transparent;
    border: 0;
    position: absolute;
    top: 0;
    bottom: 0;
    left: -10rem;
}
