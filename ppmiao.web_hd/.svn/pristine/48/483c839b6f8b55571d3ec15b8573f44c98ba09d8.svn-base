}

.dfw .random-pic {
    background-image: url(../img/random/dice_1.png);
    background-size: 100%;
    background-repeat: no-repeat;
    height: 3rem;
    width: 3rem;
    position: absolute;
    top: 2.5rem;
    left: 2rem;
}

.dfw .random-pic.initial {
    background-image: url(../img/random/dice_1.png);
}

@keyframes throwing {
    0% {
        background: url(../img/random/dice_action_0.png) no-repeat;
        background-size: 100%;
    }
    33% {
        background: url(../img/random/dice_action_1.png) no-repeat;
        background-size: 100%;
    }
    66% {
        background: url(../img/random/dice_action_2.png) no-repeat;
        background-size: 100%;
    }
    100% {
        background: url(../img/random/dice_action_3.png) no-repeat;
        background-size: 100%;
    }
}
@-webkit-keyframes throwing {
    0% {
        background: url(../img/random/dice_action_0.png) no-repeat;
        background-size: 100%;
    }
    33% {
        background: url(../img/random/dice_action_1.png) no-repeat;
        background-size: 100%;
    }
    66% {
        background: url(../img/random/dice_action_2.png) no-repeat;
        background-size: 100%;
    }
    100% {
        background: url(../img/random/dice_action_3.png) no-repeat;
        background-size: 100%;
    }
}

@-moz-keyframes hammer1
/* Firefox */

{
    0% {
        background: url(../img/random/dice_action_0.png) no-repeat;
        background-size: 100%;
    }
    33% {
        background: url(../img/random/dice_action_1.png) no-repeat;
        background-size: 100%;
    }
    66% {
        background: url(../img/random/dice_action_2.png) no-repeat;
        background-size: 100%;
    }
    100% {
        background: url(../img/random/dice_action_3.png) no-repeat;
        background-size: 100%;
    }
}

@-o-keyframes hammer1
/* Opera */

{
    0% {
        background: url(../img/random/dice_action_0.png) no-repeat;
        background-size: 100%;
    }
    33% {
        background: url(../img/random/dice_action_1.png) no-repeat;
        background-size: 100%;
    }
    66% {
        background: url(../img/random/dice_action_2.png) no-repeat;
        background-size: 100%;
    }
    100% {
        background: url(../img/random/dice_action_3.png) no-repeat;
        background-size: 100%;
    }
}

.dfw .random-pic.throwing {
    animation: throwing 0.5s infinite backwards;
    -moz-animation: throwing 0.5s infinite backwards;
    /* Firefox */
    -webkit-animation: throwing 0.5s infinite backwards;
    /* Safari ï¿½ï¿½ Chrome */
    -o-animation: throwing 0.5s infinite backwards;
    /* Opera */
}


