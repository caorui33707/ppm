.header {
    width: 100%;
    height: 40px;
    background-color: #141d26;
}

.middle {
    width: 1100px;
    height: auto;
    margin: 0 auto;
}

.header_left {
    width: auto;
    height: auto;
    float: left;
}

.header_left span {
    color: #fff;
}

.header_right {
    width: auto;
    float: right;
    z-index: 999;
    height: 40px;
    line-height: 40px;
        cursor: pointer;
}

.rel {
    position: relative !important;
}

.header_right img {
    vertical-align: middle;
}

.header_right a {
    display: inline-block;
    font-size: 14px;
    color: #fff;
    line-height: 18px;
    text-decoration: none;
}

.help span {
    margin-left: 8px;
}

.hidden {
    display: none;
}

.fl {
    float: left;
}

.devision {
    color: #fff;
    margin-left: 10px;
}

.content {
    position: absolute;
    opacity: 0.7;
    z-index: 3;
    width: 100%;
}

.clear {
    clear: both;
}

.top {
    width: 100%;
    height: 96px;
    background-color: #fff;
    opacity: 10;
}

.topright {
    width: auto;
    height: auto;
    float: right;
}
.topright ul li:hover {
    border-bottom: 2px solid #ff580f;
}





