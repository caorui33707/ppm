/*导航条*/
.middle{ width:1100px; height:auto; margin:0 auto;}
.header{ width:100%; height:40px; background-color:#141d26;}
.header_left{ width:auto; height:auto; float:left;}
.header_left img{ vertical-align:middle;}
.header_left span{ display:inline-block; font-size:14px; color:#fff; line-height:40px;}
.header_right{ width:auto; height:auto; float:right;z-index:999;}
.header_right img{ vertical-align:middle;}
.index_code{top:1px;left:0;}
.header_right a{ display:inline-block; font-size:14px; color:#fff; line-height:18px; text-decoration:none;}
.help{ border-left:1px solid #fff; height:16px; margin-top:12px; margin-left:10px;}
.help span{margin-left:8px;}
.content{ position:absolute;opacity:0.7;z-index:3; width: 100%;filter:alpha(opacity=70);}
.about_banner{margin-top:-96px;}<!--第二导航条透明度-->

.top{ width:100%; height:96px; background-color:#fff; }
.topleft{ width:auto; height:auto; float:left;color:#000;}
.top1 img{padding-top:20px;}
.top2 img{padding-top:21px;margin-left:10px;}
.top3 img{padding-top:25px;padding-left:10px;}
.top big{ display:inline-block; font-size:18px; color:#333; margin-top:30px; margin-left:20px;}
.top small{ display:inline-block; font-size:12px; color:#666; margin-left:20px;}
.topright{ width:auto; height:auto; float:right;}
.topright ul li{ float:left; list-style:none; margin-top:27px; text-align:center; margin-right:10px;}
.topright ul li a{ display:inline-block; font-size:16px; width:100px; height:35px; line-height:30px; color:#000; text-decoration:none;}


.index_hot{right:30.5%;}
.index_mount{width:1200px;height:221px;border:solid 1px #aaaaaa;margin-top:60px;}
.index_platform img{width:100%;margin-top:70px;}
.index_week p{margin-top:60px;padding-bottom:10px;}
.index_interest img{padding-left:35%;padding-top:5px;padding-right:10px;}
.index_money span{font-size:25px;color:#ff580f;}
.index_money p{font-size:10px;}
.index_lineright{border-right:solid 1px #CCC;}
.index_register{margin-top:50px;}
.index_register span{font-size:25px;color:#ff580f;}
.index_new{width:130px;line-height:25px;background-color:#898686;color:#FFF;margin-top:20px;}


.index_download{width:850px;}
.index_iphone {width:250px;padding-top:40px;}
.index_iphone p{line-height:25px;}
.index_p img{padding-bottom:10px;}
.index_android img{padding-left:20px;padding-top:20px;}
.rightcompany {padding-top:40px;}
.index_brief{width:100px;}
.index_brief p{padding-top:20px;padding-left:30px;}
.index_brief span{color:#4f5358;}
.index_connect{width:400px;padding-top:40px;}
.index_number p{padding-top:20px;padding-left:40px;}
.index_number img{padding-top:27px;padding-left:40px;padding-right:10px;}
.index_number a{color:#4f5358;}
.index_number span{font-size:30px;color:#FFF;}
.about_banner img{width:100%;}
.about_us{width:1000px;}
.about_we p{font-size:30px;color:#ff580f;padding-top:60px;}
.about_born p{color:#000;font-size:15px;text-indent:2em;}
.about_line{width:200px;height:2px;background-color:#ff580f;}
.about_what{padding-top:10px;padding-bottom:10px;}
.about_born img{margin-top:30px;margin-bottom:30px;}
.business img{padding-left:30px;}
.business p{padding-bottom:20px;padding-left:0px;}
.zero p{font-size:31px;font-weight:bold;padding-top:120px;margin-bottom:10px;}
