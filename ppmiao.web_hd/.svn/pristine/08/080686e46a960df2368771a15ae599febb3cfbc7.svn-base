.security_guarantee img{margin-top:20px;margin-bottom:20px;padding-left:50px;}
.company p{font-size:13px;color:#898686;}
.bank{width:150px;line-height:25px;border-radius:50px;border:solid 1px #000;margin-top:10px;margin-right:10px;margin-bottom:100px;}
.security_trusteeship {width:1100px;padding-top:40px;padding-bottom:40px;}
.security_trusteeship img{padding-bottom:40px;}
.security_have{padding-left:90px;padding-top:25px;}

