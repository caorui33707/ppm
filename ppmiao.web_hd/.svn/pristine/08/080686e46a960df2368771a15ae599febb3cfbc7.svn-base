/***************新增切换效果**********************/

.submit-slideTxtBox{ width:1200px;margin:20px auto; border:1px solid #ddd; text-align:left; z-index:100; }
.submit-slideTxtBox .hd{ height:70px; line-height:70px; background:#f4f4f4; padding:0 10px 0 20px; position:relative; }
.submit-slideTxtBox .hd ul{ float:left;  position:absolute; left:0px; top:-1px; height:35px;   }
.submit-slideTxtBox .hd ul li{ float:left; padding:0 40px; cursor:pointer;  }
.submit-slideTxtBox .hd ul li.on{ height:70px;  background:#fff; border-top:3px solid #ff6600; border-bottom:2px solid #fff; }
.submit-slideTxtBox .bd{min-height:500px; background:#fff;}
.submit-slideTxtBox .bd ul{ padding:15px;  zoom:1; }
.submit-slideTxtBox .bd li{ min-height:34px; line-height:27px;   }
.submit-slideTxtBox .bd li img{width:90%;display:block}
.submit-slideTxtBox .bd li .date{ float:right; color:#999;  }
