.main {
    background-color: #F5F5F5;
    margin-top: 96px;
}

.container {
    width: 1200px;
    height: auto;
    margin: 0 auto;
    padding: 40px 0 100px 0;
}

.product-data {
    background-color: #fff;
    padding: 40px 20px;
    border: 1px solid #d9d9d9
}

.product-data__title {
    font-size: 16px;
    color: #333;
    padding: 0 0 20px 0;
    border-bottom: 1px solid #d9d9d9;
}

.product-data__details {
    background-color: #F2F2F2;
    color: #333;
    font-size: 14px;
    font-weight: 450;
    line-height: 55px;
    height: 55px;
    padding: 0 40px;
    margin: 20px 0;
}

.product-data__money {
    padding: 0 10px 20px 10px;
    color: #666;
    font-size: 14px;
    line-height: 60px;
    border-bottom: 1px solid #d9d9d9;
}

.product-data__money .num {
    font-size: 16px;
    color: #333;
}

.product-data__money .sign {
    position: relative;
    right: 36px;
    bottom: 4px;
}

.discount-coupon__choose {
    padding: 0 10px;
    height: 60px;
    line-height: 60px;
    color: #666;
    font-size: 14px;
    /* border-bottom: 1px solid #d9d9d9; */
}

.discount-coupon__choose button {
    width: 120px;
    height: 30px;
    color: #fff;
    font-size: 14px;
    background-color: #ff580f;
    border: 0;
    border-radius: 3px;
    margin-left: 15px;
}

.payfor__choose {
    padding: 10px;
    height: 100px;
    line-height: 40px;
    color: #666;
    font-size: 14px;
    border-bottom: 1px solid #d9d9d9;
}

.rechange {
    color: #ff580f;
    margin: 0 20px
}

.payfor__money {
    font-size: 18px;
    color: #ff580f;
}

.checkboxclick {
    border-bottom: 1px solid #d9d9d9;
    width: 100%;
    height: 85px;
    line-height: 85px;
    position: relative;
}

.checkboxclick-warning {
    position: absolute;
    top: 23px;
    color: red;
}

.checkboxclick label {
    color: #ff580f !important;
}

.rightNow {
    color: #fff;
    width: 300px;
    background-color: #ff580f;
    height: 40px;
    text-align: center;
    line-height: 40px;
    font-size: 16px;
    border-radius: 5px;
    margin: 30px 0 0 0;
}

.prize {
    background-image: url(../images/prize.png);
    background-repeat: no-repeat;
    background-size: 100%;
    width: 300px;
    height: 154px;
    margin: 20px;
}

.prize.cannot {
    color: #e6e6e6 !important;
}

.cannot .prize__money, .cannot .prize__name, .cannot .prize__num, .cannot .prize__date, .cannot .prize__active, .cannot .prize__time {
    color: #e6e6e6 !important;
}

.discount-coupon__prize {
    display: flex;
    flex-wrap: wrap;
}

.prize__money {
    position: relative;
    width: 135px;
    font-size: 30px;
    color: #ff580f;
    top: 45px;
    left: 21px;
}

.prize__name {
    color: #999;
    position: relative;
    top: 70px;
    left: 12px;
    width: 123px;
    text-align: center;
}

.prize__num {
    color: #999;
    left: 145px;
    position: relative;
    top: -25px;
    text-align: left;
    width: 161px;
}

.prize__date {
    color: #999;
    left: 145px;
    position: relative;
    text-align: left;
    width: 161px;
    top: -17px;
}

.prize__active {
    color: #999;
    left: 145px;
    position: relative;
    top: -10px;
    text-align: left;
    width: 161px;
}

.prize__time {
    color: #999;
    left: 145px;
    position: relative;
    top: -5px;
    text-align: left;
    width: 161px;
}


.dialog {
    position: fixed;
    top: 0px;
    bottom: 0px;
    left: 0px;
    right: 0px;
}

.dialog2 {
    position: fixed;
    top: 0px;
    bottom: 0px;
    left: 0px;
    right: 0px;
}
.dialog-shadow {
    background-color: #000;
    opacity: .3;
    width: 100%;
    height: 100%;
}


.dialog-container__text {
    font-size: 22px;
    text-align: center;
    position: relative;
    top: 106px;
}

.dialog-container__button {
    position: relative;
    width: 300px;
    background: #ff580f;
    margin: 0 auto;
    color: #fff;
    height: 50px;
    line-height: 50px;
    border-radius: 5px;
    text-align: center;
    font-size: 20px;
    top: 135px;
}

.dialog-button {
    width: 200px;
    height: 50px;
    position: relative;
    top: 165px;
    left: 100px;
}
.dialog-container {
    width: 800px;
    background: #fff;
    height: 800px;
    margin: -400px auto 0 auto;
    position: relative;
    top: -50%;
    border-radius: 5px;
}
.prize__active {
    color: #999;
    left: 145px;
    position: relative;
    top: -10px;
    text-align: left;
    width: 152px;
}
.prize__name {
    color: #999;
	position: relative;
	top: 70px;
	left: 21px;
	width: 123px;
	text-align: left;
}

