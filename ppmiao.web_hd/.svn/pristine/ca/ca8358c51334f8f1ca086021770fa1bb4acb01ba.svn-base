<!--点击切换加减号-->
<script>
    
    $(function () {
        $(".bd li").click(function () {
            //如果图片是mast_03.png，换成mast_10.png
            var imgUrl = $(this).children("img").attr("src");
            if(imgUrl === "{:C('STATIC_ROOT')}/new_ppmiao/images/mast_03.png") {
                $(this).children("img").attr("src", "{:C('STATIC_ROOT')}/new_ppmiao/images/mast_10.png")
                        .next().next().slideDown(1000);

            }else {
                $(this).children("img").attr("src", "{:C('STATIC_ROOT')}/new_ppmiao/images/mast_03.png")
                        .next().next().slideUp(1000);
            }
        });

    });
</script>
