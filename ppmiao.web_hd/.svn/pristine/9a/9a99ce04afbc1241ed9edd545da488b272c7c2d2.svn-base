        /*福袋动态*/
@-webkit-keyframes shake {
	0% {
		-webkit-transform: rotate(4deg) translate3d(0,0,0)
	}

	50% {
		-webkit-transform: rotate(-4deg) translate3d(0,0,0)
	}

	100% {
		-webkit-transform: rotate(4deg) translate3d(0,0,0)
	}
}



.red{
	transform-origin: 50% 0;
	-webkit-transform-origin: 50% 0;
}
.two{
	transform-origin: 50% 0%;
	-webkit-transform-origin: 50% 0%;
}
.three{
	transform-origin: 50% 0%;
	-webkit-transform-origin: 50% 0%;
}

.red.shake{
	animation:shake .4s infinite linear;
	-webkit-animation:shake .4s infinite linear;
}
.two.shake{
	animation:shake .4s infinite linear;
	-webkit-animation:shake .4s infinite linear;
}
.three.shake{
	animation:shake .4s infinite linear;
	-webkit-animation:shake .4s infinite linear;
}
