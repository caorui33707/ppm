</head>
<body>
    <section>
        <!--正文-->
        <div class="one_pic">
            <img src="{:C('STATIC_ROOT')}/activity/event20161031/images/one_01.jpg" />
            <img style="margin-top:-1px;" src="{:C('STATIC_ROOT')}/activity/event20161031/images/one_03.jpg" />
            <img style="margin-top:-1px;" src="{:C('STATIC_ROOT')}/activity/event20161031/images/one_05.jpg" />
